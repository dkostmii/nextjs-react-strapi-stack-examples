{
  "collectionName": "components_shared_seos",
  "info": {
    "displayName": "Seo",
    "description": ""
  },
  "options": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "required": true,
      "minLength": 2
    },
    "metaDescription": {
      "type": "string",
      "required": true,
      "minLength": 2
    },
    "themeColor": {
      "type": "string",
      "default": "#000",
      "regex": "^#([0-9a-fA-F]{3}|[0-9a-fA-F]{6})$",
      "required": true,
      "maxLength": 7,
      "minLength": 4
    },
    "websiteUrl": {
      "type": "string",
      "minLength": 11,
      "regex": "^https?:\\/\\/[a-zA-Z]+(\\.[a-zA-Z]{2,})+(\\/[a-zA-Z0-9\\-]+)*\\/?$",
      "required": true
    },
    "cardImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
